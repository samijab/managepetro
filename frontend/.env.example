# Example .env file for ManagePetro Frontend
# Copy this file to .env and fill in your actual values

# Base URL for backend API (required)
VITE_API_BASE_URL=https://your-backend-url

# Default LLM model for route optimization (required)
VITE_DEFAULT_LLM_MODEL=gemini-2.5-flash

# Google Maps API key for map functionality (required)
VITE_GOOGLE_MAPS_API_KEY=your-google-maps-api-key-here

# Development mode flag (optional, for ErrorBoundary)
VITE_DEV=true

